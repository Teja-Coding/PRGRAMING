package Pratice;

import java.util.Arrays;
import java.util.Scanner;

public class programe {
	public static void main(String[] args) {
		int []arr1= {1,3,6,6,8,4,3};
		int i=0;
		int j=arr1.length-1;
		int tem;
		while(i<j) {
			tem=arr1[i];
			arr1[i]=arr1[j];
			arr1[j]=tem;
			i++;
			j--;
		}
		System.out.println(Arrays.toString(arr1));
	}
}